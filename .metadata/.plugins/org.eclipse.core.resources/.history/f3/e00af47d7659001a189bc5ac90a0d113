package com.leisgomes.cursomc.domain;

import com.leisgomes.cursomc.domain.enums.EstadoPagamento;

public class Pagamento {
	
	private Integer id;
	private EstadoPagamento tipo;
	
	private Pedido pedido;
	
	public Pagamento() {
		
	}

	public Pagamento(Integer id, EstadoPagamento tipo, Pedido pedido) {
		super();
		this.id = id;
		this.tipo = tipo;
		this.pedido = pedido;
	}
	
	
}
